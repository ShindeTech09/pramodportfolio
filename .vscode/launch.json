    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Flutter Web (Custom Address)",
            "request": "launch",
            "type": "chrome",
            "url": "http://192.168.0.102:38295",
            "webRoot": "${workspaceFolder}/lib"
        },
        {
            "name": "Flutter",
            "type": "dart",
            "request": "launch",
            "program": "lib/main.dart"
        },
        
        {
            "name": "pramod_portfolio",
            "request": "launch",
            "type": "dart"
        },
        {
            "name": "pramod_portfolio (profile mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "profile"
        },
        {
            "name": "pramod_portfolio (release mode)",
            "request": "launch",
            "type": "dart",
            "flutterMode": "release"
        }

    ]
}